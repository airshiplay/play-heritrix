<job>
    <name>chsijob</name>
    <pages>
        <url>http://yz.chsi.com.cn/sch/</url>
        <datas>
            <type>list</type>
            <xpath>
                /html/body[@class='ch-sticky']/div[@class='main-wrapper']/div[@class='container']/div[@class='yxk-table']/table[@class='ch-table']/tbody
            </xpath>
            <name>college</name>
            <items>
                <colName>collegename</colName>
                <xpath>/tr[?]/td[1]</xpath>
                <key>true</key>
            </items>
            <items>
                <colName>collegeaddress</colName>
                <xpath>/tr[?]/td[2]</xpath>
            </items>
        </datas>
    </pages>
    <pages>
        <url>http://yz.chsi.com.cn/sch/schoolInfo--schId-367890.dhtml</url>
        <datas>
            <type>list</type>
            <name>college_category</name>
            <items>
                <xpath>/html/body/div[2]/div[2]/div/h1/a</xpath>
                <colName>collegename</colName>
                <key>true</key>
            </items>
            <items>
                <xpath>//*[@id="mldm"]</xpath>
                <type>select</type>
                <colName>category</colName>
            </items>
        </datas>
        <datas>
            <type>list</type>
            <name>college_subject</name>
            <items>
                <xpath>/html/body/div[2]/div[2]/div/h1/a</xpath>
                <colName>collegename</colName>
                <key>true</key>
            </items>
            <items>
                <xpath>//*[@id='yjxkdm']</xpath>
                <type>select</type>
                <colName>subject</colName>
            </items>
        </datas>
    </pages>

</job>